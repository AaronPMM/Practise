defines =
include_dirs =
cflags = -fPIC -pthread
cflags_c =
target_out_dir = obj
target_output_name = hello

build obj/hello.hello.o: cc ../hello.c
  source_file_part = hello.c
  source_name_part = hello

build hello: link obj/hello.hello.o
  ldflags = -Wl,-rpath=\$$ORIGIN/ -Wl,-rpath-link=
  libs =
  frameworks =
  swiftmodules =
  output_extension = 
  output_dir = .
